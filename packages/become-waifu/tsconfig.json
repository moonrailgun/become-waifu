{
  "compilerOptions": {
    "target": "ES5",
    "lib": ["DOM"],
    "esModuleInterop": true,
    "isolatedModules": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "strict": false,
    "importsNotUsedAsValues": "error",
    "declaration": true,
    "resolveJsonModule": true,
    "typeRoots": ["./node_modules/@types", "../node_modules/@types", "./types"],
    "outDir": "lib",
    "baseUrl": "."
  }
}
